<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#660000</Color>
    <Color x:Key="SecondaryColor">#e60000</Color>

    <Color x:Key="FontColor">#e68a00</Color>
    
    <!--MainViewTheme-->
    <SolidColorBrush x:Key="PrimaryBrush" Opacity="0.3" Color="{StaticResource PrimaryColor}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Opacity="0.8" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="FontBrush" Color="{StaticResource FontColor}"/>
</ResourceDictionary>